{
    "name": "mosinas/laravel-youtube-analytics-service",
    "description": "Youtube Analytics wrapper package for laravel 6 support",
    "keywords": ["laravel", "google", "cloud platform"],
    "license": "MIT",
    "authors": [
        {
            "name": "mosinas",
            "email": "mosinas@yandex.ru"
        }
    ],
    "require": {
        "php": ">=7.2",
        "google/apiclient": "^2.0",
        "illuminate/support": "6.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.*",
        "mockery/mockery": "1.3.*"
    },
    "autoload": {
        "psr-4": {
            "mosinas\\GoogleService\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
